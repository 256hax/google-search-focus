{
  "manifest_version": 2,
  "name": "Google Search Focus",
  "version": "1.0.0",
  "description": "Googleの「他の人はこちらも検索」を非表示にします。",
  "icons": {
    "48": "assets/images/icons/logo.svg",
    "96": "assets/images/icons/logo.svg"
  },
  "content_scripts": [
    {
      "matches":        ["*://*/*"],
      "include_globs":  ["*://www.google.*/*"],
      "css":            ["assets/stylesheets/main.css"]
    }
  ]
}
